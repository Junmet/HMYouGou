.carousel swiper{
  width: 750rpx;
}
.carousel swiper image{
  width: 100%;
}
.navigation-list {
  display: flex;
}

.navigation-list .navigation-item {
  padding: 12rpx 30rpx;
  flex: 25%;
}

.navigation-list .navigation-item image {
  width: 100%;
  height: 140rpx;
}

commodity {
  margin-top: 34rpx;
  box-sizing: border-box;
}

.commodity .commodity-tltle {
  width: 750rpx;
  height: 59rpx;
  padding: 0 2rpx;
  margin-bottom: 10rpx;
}

.commodity .commodity-tltle image{
  width: 100%;
}

.commodity .commodity-contents {
  display: flex;
  padding: 0 15rpx;
}

.commodity-content-one {
  width: 232rpx;
  height: 386rpx;
}

.commodity-content-one image {
  height: 386rpx;
  width: 100%;
}

.commodity-content-twos {
  width: 67%;
  display: flex;
  flex-wrap: wrap;
  margin-left: 1%;
  margin-bottom: 10rpx;
}

.commodity-content-two {
  flex: 50% 0 0;
}

.commodity-content-two image {
  width: 232rpx;
  height: 188rpx;
}
