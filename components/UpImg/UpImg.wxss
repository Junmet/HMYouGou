.up_img{
    margin-top: 10rpx;
    margin-left: 10rpx;
    width: 100rpx;
    height: 100rpx;
    position: relative;
}
.up_img image{
    width: 100%;
    height: 100%;
    border-radius: 15rpx;
}
.up_img icon{
    position: absolute;
    top: 0;
    right: 0;
    margin-top: -17rpx;
    margin-right: -17rpx;
}

